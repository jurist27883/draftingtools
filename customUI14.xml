<customUI onLoad="mdlRibbon.Ribbon_OnLoad"
	xmlns="http://schemas.microsoft.com/office/2009/07/customui">
	<ribbon>
		<tabs>
			<tab id="drafting" label="起案" keytip="K">
				<group id="indent" label="インデント">
					<box id="indentLefts" boxStyle="vertical">
						<button id="indentLeft" keytip ="L1" image ="indentleft" label="全体(左)" screentip="左に全体インデント" supertip="1文字分左に段落全体をインデントします。" size="normal" onAction="mdlRibbon.IndentLeft_onAction" />
						<button id="indentPrimaryLeft" keytip ="L2" image ="indentprimaryleft" label="1行目(左)" screentip="左に1行目インデント" supertip="1文字分左に1行目をインデントします。" size="normal" onAction="mdlRibbon.IndentPrimaryLeft_onAction" />
						<button id="indentSecondaryLeft" keytip ="L3" image ="indentsecondaryleft" label="2行目以下(左)" screentip="左に2行目以下インデント" supertip="1文字分左に2行目以下をインデントします。" size="normal" onAction="mdlRibbon.IndentSecondaryLeft_onAction" />
					</box>
					<box id="indentRights" boxStyle="vertical">
						<button id="indentright" keytip ="R1" image ="indentright" label="全体(右)" screentip="右に全体インデント" supertip="1文字分右に段落全体をインデントします。" size="normal" onAction="mdlRibbon.IndentRight_onAction" />
						<button id="indentPrimaryright" keytip ="R2" image ="indentprimaryright" label="1行目(右)" screentip="右に1行目インデント" supertip="1文字分右に1行目をインデントします。" size="normal" onAction="mdlRibbon.IndentPrimaryRight_onAction" />
						<button id="indentSecondaryright" keytip ="R3" image ="indentsecondaryright" label="2行目以下(右)" screentip="右に2行目以下インデント" supertip="1文字分右に2行目以下をインデントします。" size="normal" onAction="mdlRibbon.IndentSecondaryRight_onAction" />
					</box>
					<button id="indentRound" keytip ="I" image ="indentround" label="文字単位" screentip="文字単位インデント" supertip="インデントを文字単位に設定します。" size="normal" onAction="mdlRibbon.IndentRound_onAction" />
				</group>
				<group id="style" label="スタイル">

					<gallery id="setstyle" label="段落にスタイル設定" keytip="S1" image="setstyle" size="large" showItemLabel="false" showItemImage="true" itemWidth="32" itemHeight="32" rows="2" columns="5" onAction="mdlGallery.SetStyle_onAction">
						<item id="title1" label="第1" image="title1" screentip="見出し[第1]" supertip="段落にスタイルを設定します。" />
						<item id="title2" label="1" image="title2" screentip="見出し[1]" supertip="段落にスタイルを設定します。" />
						<item id="title3" label="(1)" image="title3" screentip="見出し[(1)]" supertip="段落にスタイルを設定します。" />
						<item id="title4" label="ア" image="title4" screentip="見出し[ア]" supertip="段落にスタイルを設定します。" />
						<item id="title5" label="(ア)" image="title5" screentip="見出し[(ア)]" supertip="段落にスタイルを設定します。" />
						<item id="body1" label="第1" image="body1" screentip="本文[第1]" supertip="段落にスタイルを設定します。" />
						<item id="body2" label="1" image="body2" screentip="本文[1]" supertip="段落にスタイルを設定します。" />
						<item id="body3" label="(1)" image="body3" screentip="本文[(1)]" supertip="段落にスタイルを設定します。" />
						<item id="body4" label="ア" image="body4" screentip="本文[ア]" supertip="段落にスタイルを設定します。" />
						<item id="body5" label="(ア)" image="body5" screentip="本文[(ア)]" supertip="段落にスタイルを設定します。" />
					</gallery>
					<button id="setstyles" keytip ="S2" image="setstyles" label="選択範囲にスタイル設定" screentip="スタイルの自動設定" supertip="選択範囲にスタイルを自動設定します。" size="large" onAction="mdlRibbon.SetStyles_onAction" />
					<button id="convertlistnumbers" keytip ="S3" image="convertlistnumbers" label="箇条書き変換・解除" screentip="箇条書きの通常文字への変換/自動設定解除" supertip="自動設定された箇条書きを通常の文字に変換し、自動設定を解除します。" size="large" onAction="mdlRibbon.ConvertListNumbers_onAction" />
					<button id="clearstyle" keytip ="S4" image ="clearstyle" label="書式クリア" screentip="書式クリア" supertip="段落に設定された書式をクリアします。" size="large" onAction="mdlRibbon.ClearStyle_onAction" />
				</group>
				<group id="revision" label="変更履歴">
					<button id="acceptrevision" keytip ="H1" image ="acceptrevision" label="選択して反映" screentip="選択した履歴の反映" supertip="選択範囲にあるユーザーが加えた変更履歴を反映します。" size="large" onAction="mdlRibbon.AcceptRevisions_onAction" />
					<button id="acceptchangedformat" keytip ="H2" image ="acceptchangedformat" label="書式変更反映" screentip="書式変更履歴の反映" supertip="書式の変更履歴を反映します。" size="large" onAction="mdlRibbon.AcceptChangedFormat_onAction" />
					<toggleButton id="toggletrackingformat" keytip ="H3" image ="toggletrackingformat" label="書式変更履歴設定" screentip="書式変更履歴の設定" supertip="書式の変更履歴を設定します。" size="large" onAction="mdlRibbon.ToggleTrackingFormat_onAction" getPressed="mdlRibbon.ToggleTrackingFormat_getPressed"/>
				</group>				
				<group id="tool" label="ツール">
					<button id="changedigit" keytip ="D" image ="changedigit" label="位取り変換" screentip="位取りの変換" supertip="位取り（億・万、コンマ、なし）を順に変換します。" size="large" onAction="mdlRibbon.ChangeDigit_onAction" />
				</group>
				<group id="config" label="設定">
					<button id="configdialog" keytip ="C" image ="configdialog" label="設定" screentip="設定ダイアログ" supertip="設定ダイアログを開きます。" size="large" onAction="mdlRibbon.Config_onAction" />
					<button id="checkversion" keytip ="U" image ="checkversion" label="アップデート確認" screentip="最新版の確認" supertip="最新版がアップロードされているかを確認します。" size="large" onAction="mdlRibbon.CheckVersion_onAction" />
				</group>
			</tab>
		</tabs>
	</ribbon>
</customUI>
